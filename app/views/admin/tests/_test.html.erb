<tr>
  <td> <%= test.title %> </td>
  <td> <%= test.level %> </td>
  <% if test.category.present? %>
    <td> <%= test.category.title %> </td>
  <% end %>
  <td>
    <%= link_to t('admin.show'), admin_test_path(test) %>
    <%= link_to t('admin.edit'), edit_admin_test_path(test) %>
    <%= link_to t('admin.delete'), admin_test_path(test),
                          method: :delete,
                          data: { confirm: t('delete_confirm') } %>
  </td>
</tr>
