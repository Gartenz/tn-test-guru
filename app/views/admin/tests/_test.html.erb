<tr>
  <td> <%= test.title %> </td>
  <td> <%= test.level %> </td>
  <% if test.category.present? %>
    <td> <%= test.category.title %> </td>
  <% end %>
  <td>
    <%= link_to 'Show', admin_test_path(test) %>
    <%= link_to 'Edit', edit_admin_test_path(test) %>
    <%= link_to 'Delete', admin_test_path(test),
                          method: :delete,
                          data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
